# Environment Variables Example
# Copy this file to .env, .env.development, .env.staging, or .env.production

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Vision API (for OCR)
GOOGLE_VISION_API_KEY=your_google_vision_api_key
GEMINI_API_KEY=your_gemini_api_key

# OpenAI API (for voice notes and AI features)
OPENAI_API_KEY=your_openai_api_key

# App Environment
APP_ENV=development
DEBUG_MODE=true

# API Endpoints (if using custom backend)
API_BASE_URL=http://localhost:3000

# Sentry (Error Tracking)
SENTRY_DSN=your_sentry_dsn

# Revenue Cat (In-App Purchases)
REVENUECAT_API_KEY=your_revenuecat_api_key

# iOS In-App Purchase Secrets
# ⚠️ CRITICAL: Get this from App Store Connect
# Path: App Store Connect > Your App > App Information > Shared Secret
IOS_APP_STORE_SHARED_SECRET=your_ios_app_store_shared_secret

# Android In-App Purchase Secrets
# ⚠️ CRITICAL: Get this from Google Play Console
# Path: Google Play Console > Your App > Monetization setup > Licensing
ANDROID_PLAY_LICENSE_KEY=your_android_play_license_key_base64

# Analytics
ANALYTICS_ENABLED=false
MIXPANEL_TOKEN=your_mixpanel_token